{
    "src": "./MicrosoftWebApp/",
    "browsers": "chrome",
    "reporter": [
        "spec",
        {
            "name": "xunit",
            "output": "report.xml"
        }
    ],
    "output": "reports/report.xml",
    "screenshots": {
        "takeOnFails": false
    },
    "debugOnFail": true,
    "selectorTimeout": 3000,
    "assertionTimeout": 1000,
    "pageLoadTimeout": 1000,
    "concurrency": 1,
    "speed": 1,
    "skipJsErrors": true
}